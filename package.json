{
  "name": "sistemafrota",
  "version": "0.0.1",
  "description": "sistema para controle de frota",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm install",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "npm run test-integration && npm run test-unit && npm run test-contract",
    "start": "nodemon --exec babel-node src/server/index.js",
    "start-dashboard": "nodemon --exec nodejs-dashboard babel-node src/server/index.js",
    "test-integration": "mocha --opts src/server/test/integration/mocha.opts src/server/test/integration/**/*.js",
    "test-integration-vehicle": "mocha --opts src/server/test/integration/mocha.opts src/server/test/integration/**/vehicle.js",
    "test-integration-tire": "mocha --opts src/server/test/integration/mocha.opts src/server/test/integration/**/tires.js",
    "test-integration-driver": "mocha --opts src/server/test/integration/mocha.opts src/server/test/integration/**/driver.js",
    "test-unit": "mocha --opts src/server/test/unit/mocha.opts src/server/test/unit/**/*.js",
    "test-contract": "mocha --opts src/server/test/contract/mocha.opts src/server/test/contract/contracts/*.js",
    "vue": "vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hc3/SistemaFrota.git"
  },
  "keywords": [
    "nodejs",
    "vuejs",
    "sqlite3",
    "sequelize",
    "webschool",
    "bemean",
    "mean"
  ],
  "author": "hc3",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hc3/SistemaFrota/issues"
  },
  "homepage": "https://github.com/hc3/SistemaFrota#readme",
  "dependencies": {
    "angular-ui-bootstrap": "^2.2.0",
    "angular-ui-router": "^0.3.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "http-status": "^0.2.3",
    "jwt-simple": "^0.5.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "sequelize": "^3.24.1",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-watch": "^2.0.2",
    "browser-sync": "^2.17.3",
    "chai": "^3.5.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^3.5.0",
    "eslint-plugin-html": "^1.5.2",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-uglify": "^2.0.0",
    "jasmine-core": "^2.5.0",
    "joi": "^9.0.4",
    "joi-assert": "0.0.3",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.0.2",
    "nodejs-dashboard": "^0.1.1",
    "phantomjs": "^2.1.7",
    "supertest": "^2.0.0",
    "testdouble": "^1.6.1"
  }
}
